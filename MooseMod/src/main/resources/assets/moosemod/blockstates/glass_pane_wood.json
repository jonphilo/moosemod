{
  "multipart": [
    {   "apply": { "model": "moosemod:glass_pane_wood_post" }},
    {   "when": { "north": true },
      "apply": { "model": "moosemod:glass_pane_wood_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "moosemod:glass_pane_wood_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "moosemod:glass_pane_wood_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "moosemod:glass_pane_wood_side_alt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "moosemod:glass_pane_wood_noside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "moosemod:glass_pane_wood_noside_alt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "moosemod:glass_pane_wood_noside_alt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "moosemod:glass_pane_wood_noside", "y": 270 }
    }
  ]
}
